{
  "name": "@jowtech/codemod",
  "version": "1.1.0",
  "description": "Simple set of codemod transformations",
  "bugs": {
    "url": "https://github.com/jowapp/codemod/issues"
  },
  "homepage": "https://github.com/jowapp/codemod#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jowapp/codemod.git"
  },
  "keywords": [
    "codemod",
    "react",
    "jscodeshift",
    "transform"
  ],
  "author": "jow",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "prepublish": "yarn test:run",
    "release": "standard-version"
  },
  "dependencies": {
    "glob": "^10.3.12",
    "jscodeshift": "^0.15.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "vitest": "^1.6.0"
  },
  "bin": {
    "codemod": "bin/codemod.js"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier -w CHANGELOG.md"
    },
    "releaseCommitMessageFormat": "release: {{currentTag}}\n",
    "types": [
      {
        "type": "build",
        "hidden": true
      },
      {
        "type": "chore",
        "section": "Misc Changes"
      },
      {
        "type": "ci",
        "section": "Misc Changes"
      },
      {
        "type": "docs",
        "section": "Misc Changes"
      },
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Misc Changes"
      },
      {
        "type": "refactor",
        "section": "Misc Changes"
      },
      {
        "type": "release",
        "hidden": true
      },
      {
        "type": "revert",
        "section": "Bug Fixes"
      },
      {
        "type": "style",
        "section": "Misc Changes"
      },
      {
        "type": "test",
        "section": "Misc Changes"
      }
    ]
  }
}
